print_requests() {
    jq --sort-keys 'select(.method != "GET" and (.path | contains("/pipelines")))' < out.requests.txt
    rm out.requests.txt
}

trace $ENTRYPOINT deploy
trace $ENTRYPOINT run
rm out.requests.txt

title "Stopping pipeline"
trace $ENTRYPOINT stop
print_requests

title "Stopping pipeline with KEY, expect same output as without KEY"
trace $ENTRYPOINT stop my_pipeline
print_requests
