print_requests() {
    jq --sort-keys 'select(.method != "GET" and (.path | contains("/pipelines")))' < out.requests.txt
    rm out.requests.txt
}

trace $CLI pipelines deploy
trace $CLI pipelines run
rm out.requests.txt

title "Stopping pipeline"
trace $CLI pipelines stop
print_requests

title "Stopping pipeline with KEY, expect same output as without KEY"
trace $CLI pipelines stop my_pipeline
print_requests
