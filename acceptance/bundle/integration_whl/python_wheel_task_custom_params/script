export SPARK_VERSION=13.3.x-snapshot-scala2.12
export PYTHON_WHEEL_WRAPPER=false
envsubst < $TESTDIR/../python_wheel_task/input.json.tmpl > input.json
cat input.json
$CLI bundle init $TESTDIR/../python_wheel_task --config-file input.json
trace $CLI bundle deploy
trap "errcode trace $CLI bundle destroy --auto-approve" EXIT
trace $CLI bundle run some_other_job --python-params param1,param2
