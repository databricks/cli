envsubst < databricks.yml.tmpl > databricks.yml

trace $CLI bundle deploy

experiment_id=$($CLI bundle summary --output json | jq -r '.resources.experiments.my_experiment.id')

trace $CLI experiments get-experiment $experiment_id | jq '{name: .experiment.name, description: .experiment.description, lifecycle_stage: .experiment.lifecycle_stage}'

trace $CLI bundle destroy --auto-approve

trace $CLI bundle summary
