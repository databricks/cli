PREFIX="prefix-" envsubst.py < databricks.yml.tmpl > databricks.yml
trace grep prefix: databricks.yml
trace $CLI bundle validate -o json | jq .resources

PREFIX="[prefix]" envsubst.py < databricks.yml.tmpl > databricks.yml
trace grep prefix: databricks.yml
trace $CLI bundle validate -o json | jq .resources

PREFIX="" envsubst.py < databricks.yml.tmpl > databricks.yml
trace grep prefix: databricks.yml
trace $CLI bundle validate -o json | jq .resources

rm databricks.yml
