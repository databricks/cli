bundle:
  name: test-pipeline-recreate-$UNIQUE_NAME

workspace:
  root_path: ~/.bundle/$UNIQUE_NAME

resources:
  pipelines:
    foo:
      name: test-pipeline-$UNIQUE_NAME
      libraries:
        - notebook:
            path: ./nb.sql
      development: true
      catalog: main
      target: ${resources.schemas.bar.id}

  schemas:
    bar:
      name: test-schema-$UNIQUE_NAME
      catalog_name: main
      comment: This schema was created from DABs

targets:
  development:
    default: true
