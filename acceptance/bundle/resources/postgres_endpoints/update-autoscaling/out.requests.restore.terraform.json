{
  "method": "GET",
  "path": "/api/2.0/postgres/projects/test-pg-proj-[UNIQUE_NAME]"
}
{
  "method": "GET",
  "path": "/api/2.0/postgres/projects/test-pg-proj-[UNIQUE_NAME]/branches/main"
}
{
  "method": "GET",
  "path": "/api/2.0/postgres/[MY_ENDPOINT_ID]"
