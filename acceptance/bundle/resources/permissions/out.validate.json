{
  "bundle": {
    "environment": "default",
    "git": {
      "bundle_root_path": "."
    },
    "name": "many-permissions-test",
    "target": "default"
  },
  "resources": {
    "jobs": {
      "barj": {
        "deployment": {
          "kind": "BUNDLE",
          "metadata_file_path": "/Workspace/Users/[USERNAME]/.bundle/many-permissions-test/default/state/metadata.json"
        },
        "edit_mode": "UI_LOCKED",
        "format": "MULTI_TASK",
        "max_concurrent_runs": 1,
        "name": "job bar ${resources.jobs.fooj.name}",
        "permissions": [
          {
            "level": "CAN_VIEW",
            "user_name": "viewer1@example.com"
          },
          {
            "group_name": "data-team",
            "level": "CAN_MANAGE"
          }
        ],
        "queue": {
          "enabled": true
        },
        "tasks": [
          {
            "notebook_task": {
              "notebook_path": "/Workspace/Users/user@example.com/notebook",
              "source": "WORKSPACE"
            },
            "task_key": "main"
          }
        ]
      },
      "fooj": {
        "deployment": {
          "kind": "BUNDLE",
          "metadata_file_path": "/Workspace/Users/[USERNAME]/.bundle/many-permissions-test/default/state/metadata.json"
        },
        "edit_mode": "UI_LOCKED",
        "format": "MULTI_TASK",
        "max_concurrent_runs": 1,
        "name": "job foo",
        "permissions": [
          {
            "level": "CAN_VIEW",
            "user_name": "viewer@example.com"
          },
          {
            "group_name": "data-team",
            "level": "CAN_MANAGE"
          }
        ],
        "queue": {
          "enabled": true
        },
        "tasks": [
          {
            "notebook_task": {
              "notebook_path": "/Workspace/Users/user@example.com/notebook",
              "source": "WORKSPACE"
            },
            "task_key": "main"
          }
        ]
      }
    },
    "pipelines": {
      "barp": {
        "channel": "CURRENT",
        "deployment": {
          "kind": "BUNDLE",
          "metadata_file_path": "/Workspace/Users/[USERNAME]/.bundle/many-permissions-test/default/state/metadata.json"
        },
        "edition": "ADVANCED",
        "name": "pipeline bar ${resources.pipelines.foop.name}",
        "permissions": [
          {
            "level": "CAN_VIEW",
            "user_name": "viewer1@example.com"
          },
          {
            "group_name": "data-team",
            "level": "CAN_MANAGE"
          }
        ]
      },
      "foop": {
        "channel": "CURRENT",
        "deployment": {
          "kind": "BUNDLE",
          "metadata_file_path": "/Workspace/Users/[USERNAME]/.bundle/many-permissions-test/default/state/metadata.json"
        },
        "edition": "ADVANCED",
        "name": "pipeline foo ${resources.jobs.barj.name}",
        "permissions": [
          {
            "level": "CAN_VIEW",
            "user_name": "viewer@example.com"
          },
          {
            "group_name": "data-team",
            "level": "CAN_MANAGE"
          },
          {
            "level": "CAN_MANAGE",
            "service_principal_name": "servicebot"
          },
          {
            "level": "CAN_MANAGE",
            "user_name": "admin@databricks.com"
          }
        ]
      }
    }
  },
  "sync": {
    "exclude": [
      "out*"
    ],
    "paths": [
      "."
    ]
  },
  "workspace": {
    "artifact_path": "/Workspace/Users/[USERNAME]/.bundle/many-permissions-test/default/artifacts",
    "current_user": {
      "domain_friendly_name": "[USERNAME]",
      "id": "[USERID]",
      "short_name": "[USERNAME]",
      "userName": "[USERNAME]"
    },
    "file_path": "/Workspace/Users/[USERNAME]/.bundle/many-permissions-test/default/files",
    "resource_path": "/Workspace/Users/[USERNAME]/.bundle/many-permissions-test/default/resources",
    "root_path": "/Workspace/Users/[USERNAME]/.bundle/many-permissions-test/default",
    "state_path": "/Workspace/Users/[USERNAME]/.bundle/many-permissions-test/default/state"
  }
}
