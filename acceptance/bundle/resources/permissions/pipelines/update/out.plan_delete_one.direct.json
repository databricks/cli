{
  "plan": {
    "resources.pipelines.foo": {
      "action": "skip",
      "remote_state": {
        "creator_user_name": "[USERNAME]",
        "last_modified": [UNIX_TIME_MILLIS],
        "name": "foo",
        "pipeline_id": "[PIPELINE_ID]",
        "run_as_user_name": "[USERNAME]",
        "spec": {
          "channel": "CURRENT",
          "deployment": {
            "kind": "BUNDLE",
            "metadata_file_path": "/Workspace/Users/[USERNAME]/.bundle/permissions-test/default/state/metadata.json"
          },
          "edition": "ADVANCED",
          "id": "[PIPELINE_ID]",
          "name": "foo",
          "storage": "dbfs:/pipelines/[PIPELINE_ID]"
        },
        "state": "IDLE"
      },
      "changes": {
        "remote": {
          "storage": {
            "action": "skip",
            "reason": "server_side_default"
          }
        }
      }
    },
    "resources.pipelines.foo.permissions": {
      "depends_on": [
        {
          "node": "resources.pipelines.foo",
          "label": "${resources.pipelines.foo.id}"
        }
      ],
      "action": "update",
      "new_state": {
        "config": {
          "object_id": "/pipelines/[PIPELINE_ID]",
          "permissions": [
            {
              "permission_level": "CAN_MANAGE",
              "user_name": "viewer@example.com"
            },
            {
              "permission_level": "IS_OWNER",
              "user_name": "[USERNAME]"
            }
          ]
        }
      },
      "remote_state": {
        "object_id": "/pipelines/[PIPELINE_ID]",
        "permissions": [
          {
            "permission_level": "CAN_MANAGE",
            "user_name": "viewer@example.com"
          },
          {
            "group_name": "data-team",
            "permission_level": "CAN_MANAGE"
          },
          {
            "permission_level": "IS_OWNER",
            "user_name": "[USERNAME]"
          }
        ]
      },
      "changes": {
        "local": {
          "permissions": {
            "action": "update"
          }
        }
      }
    }
  }
}
