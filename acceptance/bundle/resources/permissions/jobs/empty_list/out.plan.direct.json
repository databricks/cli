{
  "plan_version": 2,
  "cli_version": "[DEV_VERSION]",
  "plan": {
    "resources.jobs.foo": {
      "action": "create",
      "new_state": {
        "value": {
          "deployment": {
            "kind": "BUNDLE",
            "metadata_file_path": "/Workspace/Users/[USERNAME]/.bundle/test-bundle/default/state/metadata.json"
          },
          "edit_mode": "UI_LOCKED",
          "format": "MULTI_TASK",
          "max_concurrent_runs": 1,
          "name": "job1",
          "queue": {
            "enabled": true
          },
          "tasks": [
            {
              "notebook_task": {
                "notebook_path": "/Workspace/Users/user@example.com/notebook",
                "source": "WORKSPACE"
              },
              "task_key": "main"
            }
          ]
        }
      }
    }
  }
}
