{
  "method": "PUT",
  "path": "/api/2.0/permissions/dashboards/[DASHBOARD_ID]",
  "body": {
    "access_control_list": [
      {
        "permission_level": "CAN_READ",
        "user_name": "[TEST_USER_EMAIL]"
      },
      {
        "group_name": "[TEST_GROUP_NAME]",
        "permission_level": "CAN_MANAGE"
      },
      {
        "permission_level": "CAN_MANAGE",
        "service_principal_name": "[TEST_SP_APPLICATION_ID]"
      },
      {
        "permission_level": "CAN_MANAGE",
        "user_name": "[USERNAME]"
      }
    ]
  }
}
