{
  "body": {
    "job_id": [JOB_ID],
    "new_settings": {
      "deployment": {
        "kind": "BUNDLE",
        "metadata_file_path": "/Workspace/Users/[USERNAME]/.bundle/jobs-permissions-test/default/state/metadata.json"
      },
      "edit_mode": "UI_LOCKED",
      "format": "MULTI_TASK",
      "max_concurrent_runs": 1,
      "name": "job with permissions",
      "queue": {
        "enabled": true
      },
      "tasks": [
        {
          "task_key": ""
        },
        {
          "notebook_task": {
            "notebook_path": "/Workspace/Users/user@example.com/notebook",
            "source": "WORKSPACE"
          },
          "task_key": "main"
        }
      ]
    }
  },
  "method": "POST",
  "path": "/api/2.2/jobs/reset"
}
{
  "body": {
    "access_control_list": [
      {
        "permission_level": "IS_OWNER",
        "user_name": "[USERNAME]"
      }
    ]
  },
  "method": "PUT",
  "path": "/api/2.0/permissions/jobs/[JOB_ID]"
}
