trace $CLI bundle validate
trace print_requests.py --get > out.requests.validate.$DATABRICKS_BUNDLE_ENGINE.json

trace $CLI bundle summary
trace print_requests.py --get > out.requests.summary.$DATABRICKS_BUNDLE_ENGINE.json

trace $CLI bundle plan
trace print_requests.py --get > out.requests.plan.$DATABRICKS_BUNDLE_ENGINE.json

trace $CLI bundle deploy
trace print_requests.py --get '^//telemetry-ext' > out.requests.deploy.$DATABRICKS_BUNDLE_ENGINE.json

musterr trace $CLI bundle run foo
trace print_requests.py --get > out.requests.run.$DATABRICKS_BUNDLE_ENGINE.json

trace $CLI bundle destroy --auto-approve
trace print_requests.py --get > out.requests.destroy.$DATABRICKS_BUNDLE_ENGINE.json
