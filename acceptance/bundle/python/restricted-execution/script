UV_ARGS="${UV_ARGS//\[\DATABRICKS_BUNDLES_WHEEL\]/$DATABRICKS_BUNDLES_WHEEL}"

export SOME_ENV_VAR="value_from_env"

title "Without RESTRICTED_CODE_EXECUTION, all envs are accessible"
trace uv run $UV_ARGS -q $CLI bundle summary -o json | jq '.experimental'
cat envs.txt
echo ""
rm -rf envs.txt

title "With RESTRICTED_CODE_EXECUTION=1, no envs are accessible"
trace RESTRICTED_CODE_EXECUTION=1 uv run $UV_ARGS -q $CLI bundle summary | jq '.experimental'
cat envs.txt
rm -rf envs.txt

rm -fr .databricks __pycache__
