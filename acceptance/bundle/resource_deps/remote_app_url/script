print_requests() {
    jq --sort-keys 'select(.method != "GET" and (.path | contains("/import-file/") | not))' < out.requests.txt
    rm out.requests.txt
}

print_sorted_requests() {
    jq -c < out.requests.txt | sort | jq --sort-keys 'select(.method != "GET" and (.path | contains("/import-file/") | not))'
    rm out.requests.txt
}

trace $CLI bundle validate
trace $CLI bundle plan
trace print_requests

trace $CLI bundle deploy
trace print_requests

trace $CLI bundle destroy --auto-approve
trace print_sorted_requests
