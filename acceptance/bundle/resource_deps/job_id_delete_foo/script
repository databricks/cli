echo "*" > .gitignore
trace $CLI bundle deploy
trace print_requests.py //jobs

foo_id=`read_id.py jobs foo`
echo "$foo_id:FOO_ID" >> ACC_REPLS

bar_id=`read_id.py jobs bar`
echo "$bar_id:BAR_ID" >> ACC_REPLS

title "Delete foo, keep bar"
cp only_bar.yml databricks.yml
trace $CLI bundle plan -o json > out.plan_delete.$DATABRICKS_BUNDLE_ENGINE.json
trace $CLI bundle deploy
trace print_requests.py //jobs

trace $CLI bundle destroy --auto-approve
trace print_requests.py //jobs
