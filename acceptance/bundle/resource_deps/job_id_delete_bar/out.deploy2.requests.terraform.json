{
  "method": "POST",
  "path": "/api/2.2/jobs/delete",
  "body": {
    "job_id": [BAR_ID]
  }
}
{
  "method": "POST",
  "path": "/api/2.2/jobs/reset",
  "body": {
    "job_id": [FOO_ID],
    "new_settings": {
      "deployment": {
        "kind": "BUNDLE",
        "metadata_file_path": "/Workspace/Users/[USERNAME]/.bundle/test-bundle/default/state/metadata.json"
      },
      "edit_mode": "UI_LOCKED",
      "email_notifications": {},
      "format": "MULTI_TASK",
      "max_concurrent_runs": 1,
      "name": "job foo",
      "queue": {
        "enabled": true
      },
      "run_as": {
        "user_name": "[USERNAME]"
      },
      "webhook_notifications": {}
    }
  }
}
