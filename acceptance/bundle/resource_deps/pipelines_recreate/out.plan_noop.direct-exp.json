{
  "plan": {
    "resources.jobs.bar": {
      "depends_on": [
        {
          "node": "resources.pipelines.foo",
          "label": "${resources.pipelines.foo.id}"
        }
      ],
      "action": "skip",
      "remote_state": {
        "job_id": [BAR_ID],
        "settings": {
          "deployment": {
            "kind": "BUNDLE",
            "metadata_file_path": "/Workspace/Users/[USERNAME]/.bundle/test-bundle/default/state/metadata.json"
          },
          "description": "depends on foo id [UUID]",
          "edit_mode": "UI_LOCKED",
          "format": "MULTI_TASK",
          "max_concurrent_runs": 1,
          "name": "job bar",
          "queue": {
            "enabled": true
          }
        }
      }
    },
    "resources.pipelines.foo": {
      "action": "skip",
      "remote_state": {
        "creator_user_name": "[USERNAME]",
        "last_modified": [UNIX_TIME_MILLIS],
        "name": "pipeline foo",
        "pipeline_id": "[UUID]",
        "run_as_user_name": "[USERNAME]",
        "spec": {
          "catalog": "mynewcatalog",
          "channel": "CURRENT",
          "deployment": {
            "kind": "BUNDLE",
            "metadata_file_path": "/Workspace/Users/[USERNAME]/.bundle/test-bundle/default/state/metadata.json"
          },
          "edition": "ADVANCED",
          "id": "[UUID]",
          "name": "pipeline foo"
        },
        "state": "IDLE"
      }
    }
  }
}
